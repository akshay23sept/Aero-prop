#SBATCH --job-name=Prop@10m

module purge
module load starccm/12.04

module list
export OMP_PROC_BIND=true
export OMP_NUM_THREADS=1
SLURM_NODEFILE=my_slurm_node.$$

WORKFILE=./
SIM_FILE=$WORKFILE/TestcaseCJ1at10mult.sim

srun hostname | sort > $SLURM_NODEFILE
