#!/bin/bash
#SBATCH --nodes=4
#SBATCH --ntasks=64
#SBATCH --ntasks-per-node=16
#SBATCH --time=48:00:00
#SBATCH --begin=now
#SBATCH --mail-user=Akshay.ANAND@isae-supaero.fr
